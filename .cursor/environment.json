            /* ==========================================================================
   6. GRID & CONTAINER SYSTEM
   Responsiva behållare och kolumnsystem för layout
========================================================================== */
            .container {
                width: var(--PXP-18);
                padding-right: var(--space-md);
                padding-left: var(--space-md);
                margin-right: auto;
                margin-left: auto;
                container-type: inline-size;
                container-name: main-container;
            }

            .row {
                display: flex;
