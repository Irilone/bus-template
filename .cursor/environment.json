            }

            .faq-question:focus-visible {
                outline: var(--PXR-2) solid var(--clr-accent);
                outline-offset: var(--PXR-2);
            }

            /* Lightbox */
            .img-lightbox {
                display: none;
                position: fixed;
                z-index: 9999;
                top: 0;
                left: 0;
                width: 100%;
                height: 100%;
                background-color: rgba(0, 0, 0, 0.9);
                cursor: pointer;
                animation: fadeIn var(--duration-fast) var(--ease-out);
                justify-content: center;
                align-items: center;
                transition: opacity 0.2s;
                inset: 0;
                background: rgba(0, 0, 0, 0.92);
            }

            .img-lightbox.open {
                display: flex;
                opacity: 1;
                pointer-events: auto;
            }

            .img-lightbox::before {
                content: '';
                position: absolute;
                top: 50%;
                left: 50%;
                width: 40px;
                height: 40px;
                margin: -20px 0 0 -20px;
                border: 3px solid rgba(255, 255, 255, 0.3);
                border-top-color: #fff;
                border-radius: 50%;
                animation: lightboxSpin 0.8s linear infinite;
                opacity: 0;
                transition: opacity 0.2s;
            }

            .img-lightbox.loading::before {
                opacity: 1;
            }

            .img-lightbox img,
            .img-lightbox .lightbox-img {
                position: absolute;
                top: 50%;
                left: 50%;
                transform: translate(-50%, -50%);
                max-width: 92vw;
                max-height: 85vh;
                border-radius: var(--radius-md);
                box-shadow: 0 0 24px rgba(0, 0, 0, 0.7);
                display: block;
                margin: 0 auto;
                width: auto;
                height: auto;
                object-fit: contain;
                cursor: default;
                user-select: none;
                pointer-events: none;
            }

            /* ==========================================================================
