            /* ========================================================================== 7. HEADER & NAVIGATION ========================================================================== */
            /* Icon Font Base */
            .fa,
            .fab,
            .fad,
            .fal,
            .far,
            .fas {
                -moz-osx-font-smoothing: grayscale;
                -webkit-font-smoothing: antialiased;
                -webkit-font-feature-settings: normal;
                font-feature-settings: normal;
                display: inline-block;
                font-style: normal;
                font-variant: normal;
                line-height: 1;
                text-rendering: auto;
                font-size: 1em;
                font-family: 'Font Awesome 5 Pro';
            }

            /* Main Header Container */
            .header {
                display: flex;
                flex-direction: column;
                align-items: stretch;
                height: auto;
            }

            header .container-fluid {
                width: 100%;
                max-width: 100%;
                padding-left: 0;
                padding-right: 0;
                --bs-gutter-x: 0;
                --bs-gutter-y: 0;
            }

            .navigation {
                display: flex;
                flex-direction: column;
                width: clamp(95vw, 100vw, 95vw);
                height: auto;
                padding: 0;
                margin: 0 auto;
            }

            .header .float-right,
            .header .pull-right {
                float: none;
            }

            .header [class*="position-absolute"] {
                position: relative;
            }

            /* Welcome Header */
            .welcome-header {
                display: flex;
                flex-direction: column;
                align-items: center;
                width: 100%;
                max-width: 100vw;
                margin-bottom: 16px;
                font-family: filsonpro, sans-serif;
                text-align: center;
                min-width: 0;
                box-sizing: border-box;
            }

            .welcome-title {
                font-size: 38px;
                color: var(--blue-primary);
                margin: 0 0 4px;
                font-weight: 800;
            }

            .welcome-subtitle {
                font-size: 32px;
                color: var(--blue-dark);
                margin: 0;
                font-weight: 600;
            }

            /* Navigation Logo */
            .navigation__logo {
                order: -1;
                width: 100%;
                display: flex;
                justify-content: center;
                padding: var(--space-lg) var(--space-md);
                margin-left: 0.25rem;
                box-sizing: border-box;
                height: auto;
                line-height: normal;
                -webkit-backdrop-filter: blur(10px);
                backdrop-filter: blur(10px);
            }

            .navigation__logo.flex-start {
                display: flex;
                justify-content: center;
                align-items: center;
            }

            .navigation__logo img {
                max-width: 350px;
                min-width: 225px;
                display: block;
                max-height: 40px;
                object-fit: contain;
                object-position: left top;
                width: 50vw;
            }

            .navigation__logo>a,
            .navigation__logo>img,
            .navigation__logo>svg {
                margin-right: auto;
            }

            body[class*="logo--normal"] .navigation__logo {
                height: 60px;
                padding-top: 50px;
            }

            /* Navigation Menu */
            .navigation__left,
            .navigation__left--menu {
                display: flex;
                padding-left: 50px;
                padding-right: 50px;
            }

            .navigation__left--menu,
            nav.navigation__item--has-mega-menu .navigation__item-category,
            .header-navigation-menu {
                order: -1;
                display: flex;
                flex-direction: row;
                justify-content: center;
                width: 100%;
                padding: 0 0;
                margin: 0;
                list-style: none;
                height: 60px;
                flex-wrap: nowrap; /* Förhindrar radbrytning */
                align-content: center;
                align-items: center;
            }

            .navigation__item {
                flex-grow: 1;
                flex-basis: 0;
                text-align: center;
                padding: 0 0.5rem;
                display: flex;
                align-items: center;
                justify-content: center;
                border-left: none;
                border-right: none;
            }

            button.navigation__item-category,
            a.navigation__item-category {
                color: #012363;
                font-weight: 500;
                font-size: 1rem;
            }

            .navigation__item-category {
                color: #012363;
                padding-left: 5px;
                padding-right: 5px;
                font-weight: 600;
            }

            .fal.fa-chevron-down.navigation__item-chevron--collapsed {
                color: #012363;
                font-weight: 600;
            }

            .navigation__item-chevron--collapsed,
            .navigation__item-chevron--expanded {
                font-weight: 600;
                transition: transform 0.3s ease;
            }

            .navigation__item-category[aria-expanded="true"] .navigation__item-chevron--collapsed {
                transform: rotate(180deg);
            }

            /* Mega Menu */
            .navigation__item--has-mega-menu .navigation__item-category .navigation__item-chevron--collapsed,
            .navigation__item--has-mega-menu .navigation__item-category .navigation__item-chevron--expanded {
                font-size: .75rem;
                margin-left: .5rem;
            }

            .mega-menu__wrapper a.link {
