            footer img[alt="Praktikertjänst"] {
                max-width: 19.44rem;
                min-width: 3rem;
                height: auto;
                display: inline-block;
            }

            /* ==========================================================================
   Page Content Wrapper & Body - Responsiva marginaler och padding
========================================================================== */
            /* Emulerar margin-bottom: 0, margin-top: 1rem responsivt */
            .page__content-wrapper {
                margin-bottom: 0;
                margin-top: clamp(0.75rem, 0.625rem + 0.625vw, 1rem);
