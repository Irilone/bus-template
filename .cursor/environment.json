            /* ==========================================================================
   FAQ Grid Layout - Exakt 2 rader med balanserad bredd
========================================================================== */
            .faq-item summary {
                /* Grid layout för exakt 2 rader */
                display: grid;
                grid-template-columns: 1fr auto;
                grid-template-rows: 1fr 1fr;
                align-items: start;
                gap: 0 var(--space-sm);
            }

            /* FAQ frågetext som grid child */
            .faq-item summary::before {
                content: attr(data-question);
                grid-column: 1;
                grid-row: 1 / -1;
                align-self: center;
                line-height: 1.4;
                word-break: break-word;
                hyphens: auto;
                display: -webkit-box;
                -webkit-line-clamp: 2;
                line-clamp: 2;
                -webkit-box-orient: vertical;
                overflow: hidden;
                text-overflow: ellipsis;
            }

            /* Chevron i grid */
            .faq-item summary::after {
                grid-column: 2;
                grid-row: 1 / -1;
                align-self: center;
            }

            /* ==========================================================================
   Vaccine Information Progressive Disclosure Styles
   ========================================================================== */

            /* Vaccine Categories */
            .vaccine-category {
                margin-bottom: var(--CLAMP-12);
