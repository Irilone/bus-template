            }

            .vaccine-priority .vaccine-badge {
                background: var(--clr-accent);
                color: var(--clr-on-accent);
                font-size: var(--fluid-xs);
                font-weight: var(--fw-medium);
                padding: 0.25rem 0.5rem;
                border-radius: var(--radius-xs);
                margin-left: auto;
            }

            /* Health Service Item */
            .health-service {
                border-left: 4px solid var(--clr-secondary);
            }

            /* Vaccine Summary */
            .vaccine-summary {
                display: flex;
                align-items: center;
                justify-content: space-between;
                padding: var(--CLAMP-5) var(--CLAMP-6);
                cursor: pointer;
                font-weight: var(--fw-medium);
                list-style: none;
                position: relative;
            }

            .vaccine-summary::-webkit-details-marker {
                display: none;
            }

            .vaccine-summary::after {
                content: '›';
                font-size: var(--fluid-sm);
                color: var(--clr-primary);
                transition: transform 0.2s ease;
                flex-shrink: 0;
                margin-left: var(--CLAMP-4);
            }

            .vaccine-item[open] .vaccine-summary::after {
                transform: rotate(90deg);
            }

            .vaccine-desc {
                font-size: var(--fluid-sm);
                color: var(--clr-body-secondary);
                font-weight: var(--fw-normal);
                font-style: italic;
            }

            /* Vaccine Content */
            .vaccine-content {
                padding: 0 var(--CLAMP-6) var(--CLAMP-6) var(--CLAMP-6);
                border-top: 1px solid var(--clr-border-subtle);
                background: var(--clr-bg);
            }

