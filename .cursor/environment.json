            @media (max-width: 575.98px) {

                /* Re-order content for mobile
      Ordning: 1. Intro, 2. Platsschemat, 3. Prislista, 4. Vaccininformation, 5. FAQ */
                html {
                    overflow-x: hidden;
                }

                body {
                    width: 100%;
                    max-width: 100vw;
                    overflow-x: hidden;
                    margin: 0;
                    padding: 0;
                }


                /* Fix all containers to respect viewport width */
                body * {
                    box-sizing: border-box;
                }

                /* Force proper viewport scaling and prevent zoom */
                html {
                    font-size: 16px;
                    -webkit-text-size-adjust: 100%;
                    -ms-text-size-adjust: 100%;
                    text-size-adjust: 100%;
                    zoom: 1;
                    transform: scale(1);
                    position: relative;
                    left: 0;
                    right: 0;
                }

                /* Force viewport to start at left edge */
                body {
                    position: relative;
                    left: 0;
                    right: 0;
                    margin: 0;
                    padding: 0;
                    transform: translateX(0);
                }

                /* Prevent any element from causing horizontal scroll and fix left cutoff */
                body,
                html {
                    margin: 0;
                    padding: 0;
                }


                .welcome-header,
                .navigation,
                .main-content,
                .page-content,
                main,
                section,
                article {
                    max-width: 100vw;
                    overflow-x: hidden;
                    box-sizing: border-box;
                }

                /* Ensure content doesn't get cut off on left */
                .page__content--left-align,
                article.page__content--left-align {
                    padding-left: 15px;
                    padding-right: 15px;
                    margin-left: 0;
                    margin-right: 0;
                }

                /* Fix mobile menu - ensure it starts hidden */
                .offside-navbar {
                    width: 0;
                    display: none;
                }

                .offside-navbar--menu.child-menu {
                    display: none;
                }

                .footer__links {
                    display: none;
                }

                .navigation__left--menu,
                .header-navigation-menu {
                    display: none;
                }

                /* FORCE footer menu closed by default */
                nav.footer__link-nav ul#footer__links {
                    display: none;
                }

                nav.footer__link-nav ul#footer__links:not(.open):not(.visible):not(:target) {
                    display: none;
                    visibility: hidden;
                    opacity: 0;
                    height: 0;
                    overflow: hidden;
                }

                /* Only show mobile menu when toggled */
                .offside-navbar.show-side-nav,
                .offside-navbar.open {
                    width: 100%;
                    display: block;
                }

                .offside-navbar--menu.child-menu.open {
                    display: block;
                }

                .footer__links.visible,
                .footer__links.open,
                nav.footer__link-nav ul#footer__links.open,
                nav.footer__link-nav ul#footer__links.visible,
                nav.footer__link-nav ul#footer__links:target {
                    display: flex;
                    visibility: visible;
                    opacity: 1;
                    height: auto;
                }

                /* Fix footer layout on mobile */
                footer.footer {
                    padding: 15px;
                    margin: 0;
                    width: 100vw;
                    max-width: 100vw;
                    box-sizing: border-box;
                    overflow-x: hidden;
                }

                nav.footer__link-nav {
                    width: 100%;
                    margin: 0;
                    padding: 10px;
                    box-sizing: border-box;
                    max-width: 1320px;
                }
            }

            /* Mobile Navigation Enhancement */
            @media (max-width: 767.98px) {

                /* Show mobile toggle */
                .mobile-nav-toggle {
                    display: block;
                }

                /* Hide desktop navigation */
                nav.main-navigation,
                .desktop-navigation {
                    display: none;
                }

                /* Stack all columns */
                .row>[class*="col-"] {
                    flex: 0 0 100%;
                    max-width: 100%;
                    margin-bottom: var(--space-md);
                }

                /* Mobile-specific utilities */
                .text-center-mobile {
                    text-align: center;
                }

                .order-first-mobile {
                    order: -1;
                }

                nav.footer__link-nav ul#footer__links {
                    flex-wrap: wrap;
                    gap: 5px;
                    padding: 0;
                    margin: 0;
                    justify-content: center;
                }

                nav.footer__link-nav ul#footer__links>li.col {
                    flex: 0 1 auto;
                    margin: 2px;
                    padding: 5px 8px;
                }

                /* MOBILE ORDERING - Make main container flex */
                main,
                .main-content,
                .page-content,
                body>.container,
                body>.container-fluid {
                    display: flex;
                    flex-direction: column;
                }
            }

            /* Touch-Optimized Components */
            @media (hover: none) and (pointer: coarse) {

                /* Increase all interactive element sizes */
                button,
                a.btn,
                input[type="submit"],
                input[type="button"],
                .touch-target {
                    min-height: 44px;
                    padding: 0.75rem 1rem;
                }

                /* Remove hover effects on touch devices */
                :hover {
                    transition: none;
                }
            }

            /* Responsive Spacing System */
            @media (max-width: 575.98px) {

                /* Tighter spacing on mobile */
                .container {
                    padding-inline: var(--space-sm);
                }

                section {
                    padding-block: var(--space-lg);
                }
            }

            /* --------------------------------------------------------------------------
   Mobile tables – remove corner radius for full-width look
   ----------------------------------------------------------------------- */
            @media (max-width: 575.98px) {

                .price-table,
                .location-schedule {
                    border-radius: 0;
                }
            }

            @supports not selector(:has(*)) {
                @media (max-width: 575.98px) {

