                                                        /* Lightbox */
                                                        .img-lightbox {
                                                            display: none;
                                                            position: fixed;
                                                            z-index: 9999;
                                                            top: 0;
                                                            left: 0;
                                                            right: 0;
                                                            bottom: 0;
                                                            margin: auto;
                                                            width: 92vw;
                                                            max-width: 92vw;
                                                            height: 85vh;
                                                            max-height: 85vh;
                                                            background: rgba(0, 0, 0, 0.92);
                                                            opacity: 0;
                                                            pointer-events: none;
                                                            transition: opacity 0.2s;
                                                            inset: 0;
                                                            background: rgba(0, 0, 0, 0.92);
                                                        }

                                                        .img-lightbox.open {
                                                            display: flex;
                                                            opacity: 1;
                                                            pointer-events: auto;
                                                        }

                                                        .img-lightbox::before {
                                                            content: '';
                                                            position: absolute;
                                                            top: 50%;
                                                            left: 50%;
                                                            transform: translate(-50%, -50%);
                                                            width: 100%;
                                                            height: 100%;
                                                            background-image: url('../images/loader.gif');
