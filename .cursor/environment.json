            /* ==========================================================================
   Price Table Optimeringar - Minimerar whitespace, bibehåller 3 kolumner
========================================================================== */
                                            .price-table {
                                                /* Kompakt tabellayout */
                                                width: 100%;
                                                table-layout: fixed;
                                                border-spacing: 0;
                                            }

                                            .price-table th,
                                            .price-table td {
                                                /* Minimalt padding för kompakt layout */
                                                padding: var(--PXR-6) var(--PXR-8);
                                                vertical-align: middle;
                                            }

                                            /* Första kolumnen får mer plats för vaccinnamn */
                                            .price-table th:first-child,
                                            .price-table td:first-child {
                                                width: 26.9%;
                                                text-align: left;
                                                padding-left: var(--PXR-12);
                                            }

                                            /* Priskolumnerna får lika bredd */
                                            .price-table th:not(:first-child),
                                            .price-table td:not(:first-child) {
                                                width: 18.33%;
                                                text-align: right;
                                                padding-right: var(--PXR-12);
                                                /* PROGRESSIV VISNING FÖR MOBIL INNEHÅLL: - Details/Summary element - Expanderbara sektioner - Touch-vänlig interaktion - Animerade övergångar Optimerad informationsvisning för mobila enheter */
                                                /* ===== DETAILS/SUMMARY STYLING FÖR MOBIL ===== */
                                                details.vaccine-item {
                                                    border: 1px solid var(--clr-border, #e0e0e0);
                                                    border-radius: var(--radius-md);
                                                    margin-bottom: var(--space-sm);
                                                    background: var(--clr-bg);
                                                    transition: all 0.3s ease;
                                                }

                                                details.vaccine-item[open] {
                                                    box-shadow: 0 2px 8px rgba(0, 0, 0, 0.1);
                                                }

                                                summary.vaccine-summary {
                                                    padding: var(--space-md);
                                                    cursor: pointer;
                                                    list-style: none;
                                                    display: flex;
                                                    justify-content: space-between;
                                                    align-items: center;
                                                    font-size: var(--fluid-base);
                                                    min-height: 44px;
                                                    /* Touch target */
                                                }

                                                /* Remove default arrow */
                                                summary.vaccine-summary::-webkit-details-marker {
                                                    display: none;
                                                }

                                                /* Custom arrow indicator */
                                                summary.vaccine-summary::after {
                                                    content: '▼';
                                                    font-size: 0.8em;
                                                    transition: transform 0.3s ease;
                                                    color: var(--clr-primary);
                                                }

                                                details[open] summary.vaccine-summary::after {
                                                    transform: rotate(180deg);
                                                }

                                                /* Vaccine content styling */
                                                .vaccine-content {
                                                    padding: 0 var(--space-md) var(--space-md);
                                                    animation: slideDown 0.3s ease;
                                                }

                                                /* Visa alltid alla tre priskolumner på mobil */
                                                @media (max-width: 575.98px) {
                                                    .price-table {
                                                        font-size: var(--fluid-sm);
                                                        /* 14-16px - WCAG AA compliant */
                                                    }

                                                    /* Keep first 2 columns always visible */
                                                    .price-table th:nth-child(1),
                                                    .price-table td:nth-child(1),
                                                    .price-table th:nth-child(2),
                                                    .price-table td:nth-child(2) {
                                                        display: table-cell;
                                                        padding: var(--PXR-3) var(--PXR-4);
                                                    }

                                                    /* Hide columns 3 and 4 by default on mobile too */
                                                    .price-table th:nth-child(3),
                                                    .price-table td:nth-child(3),
                                                    .price-table th:nth-child(4),
                                                    .price-table td:nth-child(4) {
                                                        display: none;
                                                        padding: var(--PXR-3) var(--PXR-4);
                                                    }

                                                    /* Mobile toggle functionality */
                                                    body:has(#v-pen:checked) .price-table th:nth-child(3),
                                                    body:has(#v-pen:checked) .price-table td:nth-child(3) {
                                                        display: table-cell;
                                                    }

                                                    body:has(#v-stu:checked) .price-table th:nth-child(4),
                                                    body:has(#v-stu:checked) .price-table td:nth-child(4) {
                                                        display: table-cell;
                                                    }

                                                    /* Justera kolumnbredder för mobil */
                                                    .price-table th:first-child,
                                                    .price-table td:first-child {
                                                        width: 40%;
                                                        padding-left: var(--PXR-6);
                                                    }

                                                    .price-table th:not(:first-child),
                                                    .price-table td:not(:first-child) {
                                                        width: 20%;
                                                        padding-right: var(--PXR-6);
                                                    }

                                                    /* Enable two-row headers on mobile */
                                                    .price-table thead th {
                                                        font-size: var(--fluid-sm);
                                                        /* 14-16px - WCAG compliant */
                                                        line-height: 1.1;
                                                        white-space: pre-line;
                                                        vertical-align: top;
                                                    }

                                                    /* Center price column headers */
                                                    .price-table thead th:nth-child(2),
                                                    .price-table thead th:nth-child(3),
                                                    .price-table thead th:nth-child(4) {
                                                        text-align: center;

                                                        /* Mobile-specific enhancements */
                                                        @media (max-width: 767.98px) {

                                                            /* Larger touch targets on mobile */
                                                            summary.vaccine-summary {
                                                                padding: var(--space-lg) var(--space-md);
                                                                min-height: 60px;
                                                            }

                                                            /* Stack vaccine info vertically */
