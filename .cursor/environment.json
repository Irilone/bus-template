            /* ==========================================================================
   9. MAP STYLES
   Kartstilar för Google Maps inbäddning
========================================================================== */
            .map-embed-container {
                --map-radius: 12px;
                --map-shadow: 0 4px 12px rgba(1, 35, 99, 0.1);
                --map-hover-shadow: 0 7px 28px rgba(1, 35, 99, 0.18);
                --map-focus-outline-color: #007bff;
                --map-max-width: 375px;
                --map-aspect-ratio: 16/9;
                --map-vertical-margin: 16px;
                --map-border-width: 1px;
                --map-border-color: rgba(1, 35, 99, 0.15);
                --map-loading-text-color: #333;
                --map-loading-bg-color: #f0f2f5;
                --map-spinner-color: var(--map-focus-outline-color);
                --map-transition-duration: 0.25s;
                position: relative;
                width: 100%;
                max-width: var(--map-max-width);
                margin-top: var(--map-vertical-margin);
                margin-bottom: var(--map-vertical-margin);
                margin-left: 0;
                margin-right: auto;
                border-radius: var(--map-radius);
                overflow: hidden;
                box-shadow: var(--map-shadow);
                border: var(--map-border-width) solid var(--map-border-color);
                aspect-ratio: var(--map-aspect-ratio);
                background-color: var(--map-loading-bg-color);
                transition: transform var(--map-transition-duration) ease, box-shadow var(--map-transition-duration) ease, border-color var(--map-transition-duration) ease;
            }

            .map-embed-container:hover {
                transform: translateY(-4px);
                box-shadow: var(--map-hover-shadow);
                border-color: var(--map-spinner-color);
            }

            .map-embed-frame {
                position: absolute;
                top: 0;
                left: 0;
                width: 100%;
                height: 100%;
                border: 0;
                display: block;
                z-index: 1;
            }

            .map-embed-container::after {
                content: "Laddar karta...";
                position: absolute;
                top: calc(50% + 10px);
                left: 50%;
                transform: translateX(-50%);
                color: var(--map-loading-text-color);
                font-size: 13px;
                z-index: 0;
            }

            @keyframes map-spinner-rotation {
                from {
                    transform: rotate(0deg);
                }

                to {
                    transform: rotate(360deg);
                }
            }

            /* ==========================================================================
   10. PERFORMANCE OPTIMIZATIONS
   Prestandaoptimeringar för snabbare rendering
========================================================================== */
            .faq-item:not([open]) {
                content-visibility: auto;
                contain-intrinsic-size: 0 60px;
            }

            .faq-item summary::after {
                will-change: transform;
            }

            .faq-item[open] summary::after {
                will-change: auto;
            }

            @keyframes lightboxSpin {
                to {
                    transform: rotate(360deg);
                }
            }

            /* ==========================================================================
   11. MEDIA QUERIES
   Responsiva brytpunkter för olika skärmstorlekar
========================================================================== */
            /* Reduced Motion */
            @media (prefers-reduced-motion: reduce) {

                html body *,
                .faq-item,
                .faq-icon::before,
                .faq-icon::after,
                .fade-in {
                    transition: none;
                    animation: none;
                }
            }

            /* Print Styles */
            @media print {
                .faq-item details {
                    /* Force open state for print */
                    display: block !important;
                }

                .faq-item details[open] summary::after {
                    content: "";
                }

                .faq-item details > :not(summary) {
                    display: block !important;
                }

                .faq-icon {
                    display: none;
                }

                .location-schedule tr[data-upcoming]::before {
                    display: none;
                }
            }

            /* Container System - Mobile-First Bootstrap 5.3 Pattern */
            .container {
                width: 100%;
                margin-inline: auto;
                padding-inline: 1rem;
            }

            @media (min-width: 576px) {
                .container {
                    padding-inline: clamp(1rem, 4vw, 2rem);
                }
            }

            @media (min-width: 992px) {
                .container {
                    max-width: 1320px;
                }

                .location-section {
                    display: grid;
                    grid-template-columns: 1fr 2fr;
                    gap: var(--space-xl);
                    align-items: start;
                }

                .location-info {
                    text-align: left;
                }

                .faq-item {
                    transition: transform var(--duration-normal) var(--ease-out), box-shadow var(--duration-slow) var(--ease-out);
                }

                .faq-item:hover {
                    transform: translateY(calc(var(--PXR-2) * -1));
                    box-shadow: 0 var(--PXR-4) var(--PXR-12) rgba(0, 0, 0, 0.1);
                }

                .page__content-body {
                    padding-right: var(--CLAMP-110);
                }
            }

            @media (min-width: 1200px) {
                .container {
                    max-width: 1140px;
                }
            }

            @media (min-width: 1400px) {
                .container {
                    max-width: 1320px;
                }
            }

            /* Container Queries */
            @container main-container (min-width: 400px) {
                .price-box {
                    padding: var(--space-lg);
                }
            }

            @container main-container (min-width: 600px) {
