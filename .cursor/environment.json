            /* CORRECTED Specificity Comment */
            body.theme--white.accent--turquoise .mega-container .mega-menu__wrapper .mega-menu__item,
            body.theme--white.accent--turquoise .mega-container .mega-menu__wrapper .mega-menu__item a.link

            /* Specificity: (0,6,2) */
                {
